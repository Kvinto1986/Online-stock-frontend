  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "*.spec.js"
        ],
        "rules": {
          "no-unused-expressions": "off",
          "no-undef": "off"
        }
      }
    ],
    "rules": {
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true
        }
      ],
      "quote-props": [
        "error",
        "as-needed"
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "dot-notation": [
        "error"
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
